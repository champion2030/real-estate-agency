{
  "entryFile": "./src/nodes/restGateway/app.ts",
  "noImplicitAdditionalProperties": "silently-remove-extras",
  "spec": {
    "outputDirectory": "./src/nodes/restGateway/swagger/",
    "basePath": "/",
    "securityDefinitions": {
      "jwt": {
        "type": "apiKey",
        "name": "x-access-token",
        "in": "header"
      }
    },
    "yaml": false,
    "specVersion": 3,
    "specMerging": "recursive"
  },
  "routes": {
    "basePath": "/",
    "routesDir": "./src/nodes/restGateway/",
    "middleware": "express"
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"]
    }
  }
}
