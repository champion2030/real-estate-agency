{
  "entryFile": "./src/nodes/restGateway/app.ts",
  "noImplicitAdditionalProperties": "silently-remove-extras",
  "spec": {
    "outputDirectory": "./src/nodes/restGateway/swagger/",
    "basePath": "/",
    "securityDefinitions": {
      "jwt": {
        "type": "apiKey",
        "name": "x-access-token",
        "in": "header"
      }
    },
    "yaml": false,
    "specVersion": 3,
    "specMerging": "recursive",
    "spec": {
      "paths": {
        "/files/blob": {
          "post": {
            "requestBody": {
              "required": true,
              "content": {
                "multipart/form-data": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "file": {
                        "type": "string",
                        "format": "binary"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
},
  "routes": {
    "basePath": "/",
    "routesDir": "./src/nodes/restGateway/",
    "authenticationModule": "./src/nodes/restGateway/middleware/authentication.ts",
    "middleware": "express"
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"]
    }
  }
}
